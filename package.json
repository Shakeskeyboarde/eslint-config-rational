{
  "name": "eslint-config-rational",
  "version": "2.1.0",
  "description": "Minimal ESLint configurations for reducing version control noise and avoiding common mistakes.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/eslint-config-rational.git"
  },
  "keywords": [
    "eslint",
    "config",
    "typescript",
    "minimal"
  ],
  "author": "Chris Ackerman <chris@topher.land>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/Shakeskeyboarde/eslint-config-rational/issues"
  },
  "homepage": "https://github.com/Shakeskeyboarde/eslint-config-rational#readme",
  "scripts": {
    "test": "eslint test --max-warnings=0",
    "prepublishOnly": "npm test",
    "modernize": "npx npm-check-updates -i --format group"
  },
  "main": "index.js",
  "files": [
    "*.js",
    "!.*"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "eslint-plugin-import": {
      "tsconfig-paths": {
        "json5": "^2.2.2"
      }
    }
  }
}
