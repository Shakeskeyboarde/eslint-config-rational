{
  "name": "eslint-config-rational",
  "version": "3.0.0",
  "description": "Minimal ESLint configurations for reducing version control noise and avoiding common mistakes.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/eslint-config-rational.git"
  },
  "keywords": [
    "eslint",
    "config",
    "typescript",
    "minimal"
  ],
  "author": "Chris Ackerman <chris@topher.land>",
  "license": "Unlicense",
  "scripts": {
    "test": "eslint test --max-warnings=0",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm test && npm run build",
    "modernize": "npx npm-check-updates@latest -i --format group"
  },
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib/**"
  ],
  "dependencies": {
    "@eslint/eslintrc": "^2.1.0",
    "@eslint/js": "^8.44.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.40.2",
    "@types/eslint__js": "^8.42.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.14",
    "eslint": "^8.44.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "typescript": "^5.1.6"
  }
}
