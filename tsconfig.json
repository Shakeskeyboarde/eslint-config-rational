{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "node",
        "types": [],
        "resolveJsonModule": true,
        "noEmit": true,
        "importHelpers": true,
        "downlevelIteration": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "jsx": "react-jsx"
    },
    "exclude": [
        ".vscode",
        "node_modules",
        "out",
        "docs"
    ]
}